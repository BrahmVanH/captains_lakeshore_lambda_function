{
	"name": "lambda_function",
	"version": "1.0.0",
	"description": "Lambda function for Captains Lakefront Rentals Website. To replace server-based API",
	"main": "index.ts",
	"scripts": {
		"start": "nodemon",
		"start:no-check": "ts-node --transpile-only -r tsconfig-paths/register src/server.ts",
		"offline": "npm run build:lambda && serverless offline",
		"build:lambda": "npm run bundle && tsc -p lambda/tsconfig.json",
		"codegen": "graphql-codegen --config codegen.yml",
		"bundle": "ncp ./src ./lambda/bundle"
	},
	"dependencies": {
		"@apollo/server": "^4.12.2",
		"@as-integrations/aws-lambda": "^3.1.0",
		"@aws-sdk/client-s3": "^3.873.0",
		"@aws-sdk/credential-providers": "^3.873.0",
		"@aws-sdk/s3-request-presigner": "^3.873.0",
		"bcryptjs": "^3.0.2",
		"braces": "^3.0.3",
		"dotenv": "^17.2.1",
		"graphql": "^16.11.0",
		"graphql-upload": "^17.0.0",
		"jsonwebtoken": "^9.0.2",
		"mongodb": "^6.18.0",
		"mongoose": "^8.17.2",
		"multer": "^2.0.2",
		"ncp": "^2.0.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.2",
		"@graphql-codegen/introspection": "^4.0.2",
		"@graphql-codegen/typescript": "^4.0.4",
		"@graphql-codegen/typescript-resolvers": "^4.0.4",
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.19",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/mongoose": "^5.11.96",
		"@types/multer": "^2.0.0",
		"braces": "^3.0.3",
		"nodemon": "^3.1.10",
		"serverless-offline": "^13.3.3",
		"serverless-plugin-typescript": "^2.1.5",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.9.2"
	},
	"author": "Brahm Van Houzen",
	"license": "MIT"
}
